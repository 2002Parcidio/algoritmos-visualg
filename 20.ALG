algoritmo "20-"
var
      Dolar, R: Real
inicio
      Escreval("--------Converta aqui------")
      Escreval("Antes porém dizer que nesse momento 1 dolar equivale a 496.60")
      Escreva("De quanto dolar quer converter para kwanza? ")
      Leia(Dolar)
      R <- (Dolar * 496.60)
      Escreva(Dolar, " é equivalente a ", R, "Kz")

fimalgoritmo