"
var
   atual, S, idade: inteiro
   nome: caractere

inicio
   Escreval("------Cálculo da idade----------")
   Escreva("Qual é o teu nome? ")
   Leia(nome)
   Escreva("Quantos anos o Sr(a) tem? ")
   Leia(idade)
   atual <- 2022
   S <- (atual - idade)
   Escreva("Prazer em te conhecer Sr(a) ", nome, ", O(a) Sr(a) nasceu em ", S)

   
fimalgoritmo