algoritmo "9-"
var
      N: Real
      
inicio
      Escreva("Digite um número: ")
      Leia(N)
      Se (N >= 100) e (N <= 200) então
         Escreva("O número que digitou está no intervalo de 100 à 200")
      Senao
           Escreva("O número que digitou está no intervalo de 100 à 200")
      Fimse

fimalgoritmo