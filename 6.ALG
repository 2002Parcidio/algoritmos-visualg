algoritmo "6-"
var
      n1, n2: Real
inicio
      Escreva("Digite um número: ")
      Leia(n1)
      Escreva("Digite outro número: ")
      Leia(n2)
      Se (n1=n2) então
         Escreva("Os números são iguais,", n1, " é igual a ", n2)
      Senao
           Escreva("O número", n1, " é diferente de ", n2)
      Fimse

fimalgoritmo