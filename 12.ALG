algoritmo "12-"
var
    km, L, cm: Real
inicio
      Escreval("-----Calcule aqui o consumo médio do seu automove-----l")
      Escreva("Quantos km percorreu? ")
      Leia(km)
      Escreva("Quantos litros usou? ")
      Leia(L)
      Cm <- km/L
      Escreva("O consumo médio do seu carro é de ", Cm)

fimalgoritmo